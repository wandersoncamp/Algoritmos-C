#include<stdio.h>

int main ()
{
	
	int numero1, numero2, soma ;
	
	printf("Informe um numero :");
	scanf("%d", &numero1);
	printf("informe o segundo valor :");
	scanf("%d", &numero2);
		
	
		printf("A soma eh %d\n", numero1 + numero2);
		
		printf("O produto eh %d\n", numero1 * numero2 );
		
		printf("A diferenca eh %d\n", numero1 - numero2 );
		
		printf("O quociente eh %d\n", numero1 / numero2 );
		
		printf("O resto da divisao eh %d\n", numero1 % numero2 );
		
}
